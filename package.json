{
  "name": "vue-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,tsx}": [
      "vitest related --run --coverage"
    ]
  },
  "dependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "15.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}